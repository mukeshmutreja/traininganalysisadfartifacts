{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADFTutorialDataFactorymkm"
		},
		"Adlsgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Adlsgen2'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureStorageLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureStorageLinkedService'"
		},
		"D365Demo_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'D365Demo'"
		},
		"BlobMetaDataSqlConnection_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobMetaDataSqlConnection'"
		},
		"Adlsgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlsstorageaccountmkm.dfs.core.windows.net"
		},
		"D365Demo_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "mukumarm@microsoft.com"
		},
		"mkmkeyvaultconnection_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://mkmkeyvault.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADFTutorialDataFactorymkm_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Adlsgen2_accountKey": {
						"value": "[parameters('Adlsgen2_accountKey')]"
					},
					"AzureStorageLinkedService_connectionString": {
						"value": "[parameters('AzureStorageLinkedService_connectionString')]"
					},
					"D365Demo_password": {
						"value": "[parameters('D365Demo_password')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"BlobMetaDataSqlConnection_connectionString": {
						"value": "[parameters('BlobMetaDataSqlConnection_connectionString')]"
					},
					"Adlsgen2_properties_typeProperties_url": {
						"value": "[parameters('Adlsgen2_properties_typeProperties_url')]"
					},
					"D365Demo_properties_typeProperties_username": {
						"value": "[parameters('D365Demo_properties_typeProperties_username')]"
					},
					"mkmkeyvaultconnection_properties_typeProperties_baseUrl": {
						"value": "[parameters('mkmkeyvaultconnection_properties_typeProperties_baseUrl')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ADFTutorialDataFactorymkm_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ADFTutorialDataFactorymkm_ArmTemplate_0')]"
			]
		}
	]
}